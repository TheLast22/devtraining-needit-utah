<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="sys_script_client">
    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>Clear all the choices from the what needed field choice list</description>
        <field>u_what_needed</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>NeedIt Request Type Options</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if(isLoading || newValue === '') {
		return;
	}
	
	var whatneeded = g_form.getValue('u_what_needed');
	if (g_form.getValue('u_request_type') === 'hr') {
		g_form.addOption('u_what_needed', ' ', ' ');
		g_form.addOption('u_what_needed', ' ', ' ');
		g_form.addOption('u_what_needed', ' ', ' ');
   }
	if (g_form.getValue('u_request_type') === 'facilities') {
		g_form.addOption('u_what_needed', ' ', ' ');
		g_form.addOption('u_what_needed', ' ', ' ');
		g_form.addOption('u_what_needed', ' ', ' ');
   }
	if (g_form.getValue('u_request_type') === 'legal') {
		g_form.addOption('u_what_needed', ' ', ' ');
		g_form.addOption('u_what_needed', ' ', ' ');
		g_form.addOption('u_what_needed', ' ', ' ');
   }
	

	// If the form is loading and it is not a new record, set the u_what_needed value to the
    // value from the record before it was loaded
    if (isLoading && !g_form.isNewRecord()) {
        g_form.setValue('u_what_needed', whatneeded);
    }
	
}]]></script>
